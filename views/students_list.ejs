<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Students</title>
    <link rel="stylesheet" href="Attendance.css">
</head>
<body>
    <nav class="navbar">
        <img class="logo" src="img/logo.png" alt="...">
        <h3>भारतीय प्रौद्योगिकी संस्थान इंदौर <br> Indian Institute of Technology Indore</h3>
    </nav>

    <h1>Students List</h1>
    <h2>Welcome to Online Education Portal</h2>

    <div class="container">
        <table class="table">
            <tr>
                <th>Serial No.</th>
                <th>Name</th>
                <th>Program</th>
                <th>Year of study</th>
                <th>Enrolled Courses</th>
            </tr>


            <%
            if(data.length>0){
                var i=1;
                data.forEach((inf)=>{
                    %>
                    <tr>
                        <td><%= i; %></td>
                        <td><%= inf.Student_Name %></td>
                        <td><%= inf.Student_Program %></td>
                        <td><%= inf.Student_Year_of_study %></td>
                        <td><%= inf.Student_Enrolled_courses %></td>
                    </tr>
            <% i++;    })%>
            <% }else{ %>
                <td colspan="7">No Data Found</td>
            <% }%>
        </table>
    </div>
</body>
</html>
