  <h1> My Courses </h1>
 
    <%if(data.length >0){
        var i=1; %>
          <table class="table">
      <tr style="color: rgb(151, 4, 156); background-color: chartreuse;">
        <th>Serial No.</th>
        <th>Course Code</th>
        <th>Course Name</th>
        <th>Credits</th>
       
      </tr>
      <% var i =1%>
      <% data.forEach((item)=>{ %>
        <tr>
          <td><%= i %></td>
          <td><%= item.Course_Code %></td>
          <td><%= item.Course_Name %></td>
          <td><%= item.Credits %></td>
          <!-- <td><%= item.day %></td> -->
      </tr>
      <% i++}) %>
    </table>
    <%} %>

</body>

</html>
