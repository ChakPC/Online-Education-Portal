<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="container">
        <table class="table">
            <tr>
                <th>Serial No.</th>
                <th>Course Code</th>
                <th>Course Name</th>
                <th>Credits</th>
                <!-- <th>Year Of Admission</th> -->
            </tr>


            <%
            if(data.length>0){
                var i=1;
                data.forEach((inf)=>{ 
                    %>
                    <tr>
                        <td><%= i; %></td>
                        <td><%= inf.Course_Code %></td>
                        <td><%= inf.Course_Name %></td>
                        <td><%= inf.Credits %></td>
                        <!-- <td><%= inf.Year_Of_Study %></td> -->
                    </tr>
            <% i++;    })%>
            <% }else{ %>
                <td colspan="7">No Data Found</td>
            <% }%>
        </table>
    </div>
</body>
</html>