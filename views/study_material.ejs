  <h1> Study Material </h1>
  
    <%if(data.length >0){
        var i=1; %>
        <table class="table">
            <tr style="color: rgb(151, 4, 156); background-color: chartreuse;">
                <th>Serial No.</th>
                <th>Couse Code</th>
                <th>Material Link</th>
            </tr>
           <% data.forEach((item)=>{%>
                <tr>
                    <td><%= i%></td>
                    <td><%=item.Course_Code%></td>
                    <td><%=item.Link%></td>
                </tr>
            <%i++})%>
        </table>
    <%} %>

</body>

